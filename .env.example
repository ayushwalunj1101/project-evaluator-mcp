# Get your API key from: https://www.perplexity.ai/
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Optional: Logging Level
FASTMCP_LOG_LEVEL=INFO

# Optional: Python Path (usually not needed)
# PYTHONPATH=./src
